<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gray.dao.FunctionMapper">

   <select id="selectAll" resultType="Function">
      select
        *
      from
        function
      where
        status = 0
      order by
      sort desc
   </select>

</mapper>
